{
	"name": "stancy",
	"version": "1.0.0-alpha.28",
	"description": "A simple API for delivering content from static files and folders.",
	"main": "dist/index.js",
	"browser": "dist/client/index.js",
	"scripts": {
		"babel": "babel src -d dist",
		"webpack": "webpack --config webpack.config.js",
		"rollup": "rollup --config",
		"build": "npm run rollup && eslint",
		"demo": "NODE_ENV=development node src/demo.js",
		"test": "./node_modules/.bin/eslint src/ && npm run build",
		"dev": "npm run rollup && npm run demo"
	},
	"files": [
		"/dist"
	],
	"keywords": [
		"static file",
		"generator",
		"database",
		"data store",
		"flat file"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/limitlessloop/stancy.git"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@rollup/plugin-json": "^4.1.0",
		"eslint": "^7.2.0",
		"rollup": "^2.16.0",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-terser": "^6.1.0"
	},
	"dependencies": {
		"chokidar": "^3.4.0",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"front-matter": "^4.0.2",
		"json5": "^2.1.3",
		"jsonata": "^1.8.3",
		"marked": "^1.1.0",
		"node-fetch": "^2.6.0",
		"pluralize": "^8.0.0",
		"smarkt": "0.0.6",
		"yaml": "^1.10.0",
		"toml": "^3.0.0",
		"hjson": "^3.2.1"
	}
